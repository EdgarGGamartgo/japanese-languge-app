{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "test:watch": "jest --watch",
    "test": "jest",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "pub": "git add . && git commit -m \"Updates\" && git push && git status",
    "cache:clean": "npm cache clean --force && expo r -c"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "@types/react-loader-spinner": "^3.1.3",
    "@types/react-native-loading-spinner-overlay": "^0.5.2",
    "expo": "~40.0.0",
    "expo-status-bar": "~1.0.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-loader-spinner": "^4.0.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-loading-spinner-overlay": "^2.0.0",
    "react-native-web": "~0.13.12"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-native": "^7.1.0",
    "@types/jest": "^26.0.20",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.7",
    "@types/styled-components-react-native": "^5.1.1",
    "axios": "^0.21.1",
    "jest": "^26.6.3",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "^17.0.1",
    "styled-components": "^5.2.1",
    "styled-components-react-native": "^1.0.0",
    "ts-jest": "^26.5.1",
    "typescript": "~4.0.0"
  },
  "private": true
}
